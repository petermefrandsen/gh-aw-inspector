{
  "name": "gh-aw-inspector",
  "displayName": "GH Agentic Workflows Inspector",
  "description": "A VS Code extension designed to interface with the GitHub Agentic Workflows CLI (gh aw).",
  "version": "0.0.0",
  "publisher": "petermefrandsen",
  "repository": {
    "type": "git",
    "url": "https://github.com/petermefrandsen/gh-aw-inspector"
  },
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gh-aw-inspector.startEvaluation",
        "title": "GH AW: Start Evaluation",
        "icon": "$(play)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "gh-aw-inspector",
          "title": "GH Agentic Workflows",
          "icon": "resources/gh-aw-inspector.svg"
        }
      ]
    },
    "views": {
      "gh-aw-inspector": [
        {
          "type": "webview",
          "id": "gh-aw-inspector-sidebar",
          "name": "Workflows",
          "icon": "resources/gh-aw-inspector.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "tsc -p ./",
    "check-types": "tsc --noEmit",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "pnpm run check-types && node esbuild.js --production",
    "test": "vscode-test",
    "test:coverage": "vscode-test --coverage --coverage-output ./coverage --coverage-reporter text --coverage-reporter lcov --coverage-reporter cobertura",
    "test:e2e": "extest run-tests ./out/test/e2e/*.test.js test-workspace",
    "lint": "eslint src --ext ts",
    "deploy": "vsce publish"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.7.1",
    "esbuild": "^0.27.3",
    "eslint": "^8.57.1",
    "mocha": "^11.7.5",
    "npm-run-all": "^4.1.5",
    "proxyquire": "^2.1.3",
    "typescript": "^5.3.3",
    "vscode-extension-tester": "^8.22.0"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.44",
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "cronstrue": "^3.12.0",
    "gray-matter": "^4.0.3"
  }
}
